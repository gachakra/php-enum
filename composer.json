{
    "name": "gachakra/php-enum",
    "type": "vcs",
    "description": "Enum for PHP 7",
    "keywords": [
        "php",
        "enum"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.0",
        "psy/psysh": "^0.9.9"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gachakra\\PhpEnum\\": "src/",
            "Gachakra\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gachakra\\PhpEnum\\Tests\\": "tests/",
            "Gachakra\\PhpEnum\\Examples\\": "examples/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
